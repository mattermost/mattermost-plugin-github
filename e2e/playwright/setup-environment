cd ../../../
git clone https://github.com/mattermost/mattermost-server.git
cd mattermost-server
git checkout playwright-test-dir

source ~/.nvm/nvm.sh
nvm install

cd webapp
npm i

cd ../e2e/playwright

npm i
npx playwright install --with-deps

cd ../../../mattermost-plugin-github/e2e/playwright
npm i
